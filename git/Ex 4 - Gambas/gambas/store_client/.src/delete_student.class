Public Sub dsocket_Ready()
  '***********************************
  ' When connection proccess has finished
  ' successfully, "Connected" event will raise
  '***********************************
  Dim cb As String
  Print "connected"
  cb = "values"
  Write #dsocket, cb, Len(cb)
  

End

Public Sub dsocket_Read()
  Dim rollno As String
  Dim element As String[]
  Dim sub As String
  Dim allval As String
  If dsocket.Status <> Net.Connected Then Return
  Read #dsocket, rollno, Lof(dsocket)
  Print rollno
  element = Split(rollno, "/")
  For Each sub In element
    rollno_combo.Add(sub)
    Next

  

End

